{
  "npm": false,
  "git": {
    "push": false,
    "commitMessage": "[automated commit] Release v${version}",
    "tagAnnotation": "Release v${version}",
    "tagName": "v${version}"
  },
  "github": {
    "release": true,
    "releaseName": "Release v${version}"
  },
  "plugins": {
    "@release-it/bumper": {
      "in": "package.json",
      "out": "package.json"
    }
  },
  "hooks": {
    "after:git:release": "echo AFTER GIT PUSH BEFORE GITHUB RELEASE"
  }
}
